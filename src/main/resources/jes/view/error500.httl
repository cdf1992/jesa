<html>
	<head>
		<meta charset="utf-8">
		<title>500错误</title>
		<!--<meta name="viewpoint" content="width=device-width,initial-scale=1">-->
		<link rel="stylesheet" href="$!{app}/res/css/404.css" />
		<script src="$!{app}/res/js/jq/jquery-1.9.1.min.js"></script>
	</head>
	<script language="javascript">
		function show()
		{
			document.getElementById('msg').style.display ="";
		}
		function hide()
		{
			document.getElementById('msg').style.display ="none";
		}
	</script>
	<body>
		<p id="msg" style="position:fixed;z-index: 999;font-size:10px;background-color:#fff;width:100%;height:100%;display: none;">
			<a href="javascript:void(0);" onclick="hide();">返回</a><br>
			${message}
		</p>
		<div class="code">
			<p>ERROR ${status}</p>
		</div>
		<div class="road">
			<div class="shadow">
				<div class="shelt">
					<div class="head">
						<div class="eyes">
							<div class="lefteye">
								<div class="eyeball"></div>
								<div class="eyebrow"></div>
							</div>
							<div class="righteye">
								<div class="eyeball"></div>
								<div class="eyebrow"></div>
							</div>
						</div>
					</div>
				</div>
				<div class="hat"></div>
				<div class="bubble">
					<a href="javascript:void(0);" onclick="show();">Go to error?</a>
				</div>
			</div>
			<p>PAGE ${error}</p>
		</div>
	</body>
	<script type="text/javascript" src="$!{app}/res/js/404.js" ></script>
</html>
